  objs[i].mass=2.0*objs[i].radius*objs[i].radius*objs[i].radius;
  o[i].x=GR_WIDTH/2;
  o[i].y=GR_HEIGHT/2;

  i=AllocObject(OT_CARGO); //always #1
  objs[i].mass=0.5*objs[i].radius*objs[i].radius*objs[i].radius;
  o[i].x=0;
  o[i].DxDt=16.0;
  o[i].y=GR_HEIGHT/2;
