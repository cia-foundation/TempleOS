/*
I8 ArcDetermineCompressionType(U1 *src,U8 size)
{
  U8 i,j=0;
  for (i=0;i<size;i++)
    j|=src[i];
  if (j & 0x80)
    return CT_8_BIT;
  else
    return CT_7_BIT;
}
*/
	ALIGN	8,OC_NOP
CP_ARC_DETERMINE_COMPRESSION_TYPE::
	PUSH	RBP
	MOV	RBP,RSP
	PUSH	RSI
	MOV	RSI,U8 SF_PARAM1[RBP]
	MOV	RCX,U8 SF_PARAM2[RBP]
	OR	RCX,RCX
	JZ	@@100
	XOR	EBX,EBX
@@1:	LODSB
	OR	BL,AL
	LOOP	@@1
	OR	BL,BL
	JNS	@@100
	MOV	RAX,CT_8_BIT
	JMP	@@110
@@100:	MOV	RAX,CT_7_BIT
@@110:	POP	RSI
	POP	RBP
	RET
