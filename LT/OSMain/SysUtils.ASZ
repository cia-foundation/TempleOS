OR_U4_BIT_FIELD::
	PUSH	RBP
	MOV	RBP,RSP
	MOV	RBX,U8 SF_PARAM2[RBP]
	SHR	RBX,3
	ADD	RBX,U8 SF_PARAM1[RBP]
	MOV	RAX,U8 SF_PARAM3[RBP]
	MOV	RCX,U8 SF_PARAM2[RBP]
	AND	RCX,7
	SHL	RAX,CL
	OR	U8 [RBX],RAX
	POP	RBP
	RET
