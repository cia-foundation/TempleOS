COUT_JOIN::
	OR	RBX,RBX
	JZ	@@100
@@20:	MOV	RDX,U8 [RBP]
	MOV	RAX,U8 8[RBP]
	CALL	U8 COUT_JMP_TABLE[RDX*8]
	SUB	RBP,16
	DEC	RBX
	JNZ	@@20
@@100:	RET

