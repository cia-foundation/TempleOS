@@I34:
	MOV	RBP,U8 32[RBP]
	MOV	RBX,RAX 	//GET PARAM CNT
	ADD	RBX,RBX
	LEA	RBP,-16[RBP+RBX*8]
	MOV	RBX,RAX 	//GET PARAM CNT
	CALL	COUT_JOIN
	POP	RBP
	IRET


