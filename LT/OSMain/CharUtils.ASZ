CP_ADD_SYS_HASH_ENTRY::
	PUSH	RBP
	MOV	RBP,RSP
	PUSH	RSI
	PUSH	RDI
	MOV	RSI,U8 SF_PARAM1[RBP]
	MOV	RSI,U8 HTE_STR[RSI]
	MOV	RDI,U8 SF_PARAM2[RBP]
