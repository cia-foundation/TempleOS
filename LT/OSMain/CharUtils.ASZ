CP_STRSTR::
	PUSH	RBP
	MOV	RBP,RSP
	PUSH	RSI
	PUSH	RDI
	MOV	RSI,U8 SF_PARAM1[RBP]
	MOV	RDI,U8 SF_PARAM2[RBP]
	MOV	DL,U1 [RDI]
	OR	DL,DL
	JZ	@@90
	JMP	@@5
@@1:	INC	RSI
@@5:	LODSB
	OR	AL,AL
	JZ	@@100
	CMP	AL,DL
	JNE	@@5
	DEC	RSI
	MOV	RCX,1
@@10:	MOV	AL,U1 [RDI+RCX]
	OR	AL,AL
	JZ	@@90
	CMP	AL,U1 [RSI+RCX]
	JNE	@@1
	INC	RCX
	JMP	@@10

@@80:	DEC	RSI
@@90:	MOV	RAX,RSI
	JMP	@@101
@@100:	XOR	EAX,EAX
