	MOV	ECX,8
BINARY_TO_FIXED_HEX:
	PUSHFD				//-****
	CLI				//because of STD
	PUSH	EAX			//-****
	PUSH	EDX			//-****
	ADD	EDI,ECX
	PUSH	EDI			//-**** EDI WE WANT TO RETURN
	DEC	EDI
	MOV	EDX,EAX
	STD

@@1:	MOV	AL,DL
	AND	AL,0xF
	ADD	AL,0x90
