	PUSH	EDX
	XOR	EDX,EDX
	OR	ECX,ECX
	JZ	@@9
@@1:	SHL	EDX,4
	LODSB
	SUB	AL,0x30
	CMP	AL,9
	JA	@@2
	SUB	AL,7
@@2:	MOVZX	EAX,AL
	ADD	EDX,EAX
	LOOP	@@1

@@9:	MOV	EAX,EDX
	POP	EDX
	RET
