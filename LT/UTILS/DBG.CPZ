public void StackRep(U4 *esp)
{
  U8 i;
  U4 my_esp[32];
  BoolU4 old_preempt=Preempt(OFF);
  for (i=0;i<32;i++) {
    my_esp[i]=*esp;
    esp=esp+4;
  }
  Preempt(old_preempt);
  for (i=0;i<32;i++)
    PrintF("%08X [ESP+%04X]: %08X %P\r\n",my_esp+i<<2,i<<2,my_esp[i],my_esp[i]);
  CrLf;
}


