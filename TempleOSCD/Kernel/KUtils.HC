_RET::
	RET
_SET_MSR::
	PUSH	RBP
	MOV	RBP,RSP
	MOV	RAX,SF_ARG2[RBP]
	MOV	RDX,RAX
	SHR	RDX,32
	MOV	RCX,SF_ARG1[RBP]
	WRMSR
	POP	RBP
	RET1	16
_SYS_HLT::
	CLI
@@05:	HLT
	JMP	@@05
